# Docker Compose environment variables for Production
# Copy this file to .env and update with actual values

# Database Configuration
PROD_DB_ROOT_PASSWORD=change_this_root_password
PROD_DB_PASSWORD=change_this_db_password

# Application Configuration
APP_ENV=production
APP_DEBUG=false

# Docker Hub (if pushing images)
DOCKER_HUB_USERNAME=your_dockerhub_username
DOCKER_IMAGE_TAG=latest
